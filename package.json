{
  "name": "torrent-files-library",
  "version": "0.0.0-development",
  "description": "Enables to get a library of downloaded torrent files (movie or tv show) from directories",
  "main": "index.js",
  "files": [
    "index.js",
    "lib/"
  ],
  "scripts": {
    "semantic-release": "semantic-release",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha",
    "compile": "babel -d lib/ src/ --source-maps both",
    "prepare": "npm run compile",
    "test": "mocha",
    "generate-docs": "node_modules/.bin/jsdoc --configure config/.jsdoc.json --verbose",
    "lint": "./node_modules/.bin/eslint -c config/.eslintrc.js ./src ./test --fix"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=5"
  },
  "keywords": [
    "torrent",
    "library",
    "parser",
    "torrent file",
    "torrent files",
    "parse torrent",
    "parse torrent file",
    "parse torrent files",
    "parse torrent name",
    "torrent library"
  ],
  "release": {
    "analyzeCommits": {
      "preset": "eslint",
      "releaseRules": "./config/release-rules.js"
    },
    "generateNotes": {
      "preset": "eslint"
    }
  },
  "author": "jy95",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jy95/torrent-files-library.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "conventional-changelog-eslint": "^0.3.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.13.1",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.7.0",
    "istanbul": "^1.1.0-alpha.1",
    "jsdoc": "^3.5.5",
    "minami": "^1.2.3",
    "mocha": "^4.0.0",
    "semantic-release": "^11.0.2",
    "sinon": "^4.0.1"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "filehound": "^1.16.2",
    "lodash": "^4.17.4",
    "parse-torrent-title": "^1.0.0",
    "video-extensions": "^1.1.0"
  }
}
